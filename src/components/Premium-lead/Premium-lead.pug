    .premium-lead__steps
      -
        let premiumLeadContent = [
            {
                title: '#1',
                description: 'We thoroughly filter Clients and work only with IT, software, consulting, and social influence.'
            },
            {
                title: '#2',
                description: 'We have formed our own culture of hiring, engaging and continuous learning for employees. You will work with top-performing, happy people.'
